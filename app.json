{
  "expo": {
    "name": "GeoGenie",
    "slug": "GeoGenie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/geogenie_logo.png",
    "scheme": "geogenie",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vijethegde.GeoGenie"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.vijethegde.GeoGenie",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/geogenie_logo.png",
        "backgroundColor": "#E6F4FE"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/geogenie_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "820cc77f-04d0-4e5e-80f0-43612bf30727"
      }
    }
  }
}
